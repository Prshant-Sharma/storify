{
  "name": "react-components",
  "private": true,
  "version": "1.0.0",
  "description": "React wrapper for storify",
  "scripts": {
    "start": "cross-env NODE_ENV=\"development\" webpack --config webpack.js --watch",
    "compile": "cross-env NODE_ENV=\"production\" webpack --config webpack.js",
    "typings": "cp index.d.ts dist/",
    "build": "run-s compile typings",
    "test": "jest || true",
    "test:coverage": "jest --coverage || true",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
    "isready": "npm run format && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gugadev/pacifico-ui.git"
  },
  "keywords": [
    "ui",
    "react",
    "javascript"
  ],
  "author": "Gustavo Garzaki ~ Pac√≠fico Seguros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gugadev/pacifico-ui/issues"
  },
  "homepage": "https://github.com/gugadev/pacifico-ui#readme",
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.2",
